{
  "name": "fastify-cli",
  "version": "0.12.0",
  "description": "Run a fastify route with one command!",
  "main": "cli.js",
  "bin": {
    "fastify": "cli.js"
  },
  "scripts": {
    "lint": "standard",
    "unit": "tap test/*.js app_template/test/*/*.test.js",
    "test": "npm run lint && npm run unit"
  },
  "keywords": [
    "fastify",
    "cli",
    "one command"
  ],
  "author": "Tomas Della Vedova - @delvedor (http://delved.org)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fastify/fastify-cli.git"
  },
  "bugs": {
    "url": "https://github.com/fastify/fastify-cli/issues"
  },
  "homepage": "https://github.com/fastify/fastify-cli#readme",
  "engines": {
    "node": ">= 4.0"
  },
  "standard": {
    "ignore": [
      "test/data/parsing-error.js",
      "test/data/undefinedVariable.js"
    ]
  },
  "dependencies": {
    "commist": "^1.0.0",
    "fastify": "^1.1.1",
    "generify": "^2.1.0",
    "help-me": "^1.1.0",
    "minimist": "^1.2.0",
    "pino-colada": "^1.4.4",
    "pump": "^3.0.0",
    "resolve-from": "^4.0.0",
    "update-notifier": "^2.3.0"
  },
  "devDependencies": {
    "fastify-autoload": "^0.2.0",
    "fastify-plugin": "^0.2.2",
    "mkdirp": "^0.5.1",
    "pre-commit": "^1.2.2",
    "request": "^2.83.0",
    "rimraf": "^2.6.2",
    "standard": "^11.0.0",
    "tap": "^11.1.2",
    "walker": "^1.0.7"
  }
}
